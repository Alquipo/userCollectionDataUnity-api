              "schema": {
                "type": "object",
                "properties": {
                  "nickname": {
                    "type": "string"
                  },
                  "scene": {
                    "type": "number"
                  },
                  "q1": {
                    "type": "array"
                  },
                  "q2": {
                    "type": "array"
                  },
                  "q3": {
                    "type": "array"
                  },
                  "q4": {
                    "type": "array"
                  },
                  "q5": {
                    "type": "array"
                  }
                },
                "example": {
                  "nickname": "nickName sample",
                  "scene": 10
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User created"
          },
          "403": {
            "description": "User already exists"
          }
        }
      },
